class OperasiMatematika {
    public static void main(String[] args) {
        int x = 22, y = 33;
        System.out.println("Penjumlahan: " + (x + y));
        System.out.println("Pengurangan: " + (x - y));
        System.out.println("Perkalian: " + (x * y));
        System.out.println("Pembagian: " + ((double) x / y));
    }
}
